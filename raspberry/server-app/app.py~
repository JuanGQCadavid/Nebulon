import socket

# Conecction Type,TCP Connection
s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)

host = ''
port = 5555

try:
    s.bind((host,port))
except socket.error as e:
    print(str(e))

s.listen(5)

while True:
    conn, addr = s.accept()
    print('Connected to: ' + addr[0] + ":"+ str(addr[1]))

